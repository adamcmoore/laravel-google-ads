{
  "name": "adamcmoore/laravel-google-ads",
  "type": "library",
  "description": "Google Ads API for Laravel 4.2",
  "minimum-stability": "dev",
  "keywords": [
    "laravel",
    "ads",
    "adwords",
    "google"
  ],
  "homepage": "https://github.com/adamcmoore/laravel-google-ads",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj Petersen",
      "email": "nikolaj.lovenhardt@gmail.com",
      "homepage": "http://github.com/nikolajlovenhardt"
    },
    {
      "name": "Adam Moore",
      "email": "adam@acmoore.co.uk",
      "homepage": "http://github.com/adamcmoore"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "ext-soap": "*",
    "laravel/framework": "4.2.*",
    "googleads/googleads-php-lib": "~32.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "codeclimate/php-test-reporter": "dev-master",
    "phpmd/phpmd" : "1.4.*",
    "mockery/mockery": "~0.9.4"
  },
  "autoload": {
    "psr-4": {
      "LaravelGoogleAds\\": "src/LaravelGoogleAds"
    },
    "files": [
      "src/LaravelGoogleAds/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelGoogleAdsTests\\": "tests/"
    }
  }
}
